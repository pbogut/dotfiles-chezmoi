layout {
    default_tab_template {
        children
        pane size=1 borderless=true {
            plugin location="zellij:compact-bar"
        }
    }
    tab name="nvim" focus=true {
        pane command="nvim-godot"
    }
    tab name="godot" {
        pane command="godot" {
            args "project.godot"
        }
    }
}
