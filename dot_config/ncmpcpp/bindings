# @see /usr/share/doc/ncmpcpp/bindings
def_key "_"
    find
def_key "_"
    find_item_forward

def_key "+"
    show_clock
def_key "="
    volume_up

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-u"
    page_up
#push_characters "kkkkkkkkkkkkkkk"
def_key "ctrl-d"
    page_down
#push_characters "jjjjjjjjjjjjjjj"

def_key "h"
    previous_column
def_key "l"
    next_column

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

# not used but bound
def_key "J"
    move_sort_order_down
def_key "K"
    move_sort_order_up

def_key "g"
    move_home

def_key "G"
    move_end

def_key "t"
    jump_to_position_in_song

def_key "space"
    select_item
def_key "D"
    delete_playlist_items
